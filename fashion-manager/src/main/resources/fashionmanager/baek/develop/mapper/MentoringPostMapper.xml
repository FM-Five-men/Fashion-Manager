<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="fashionmanager.baek.develop.mapper.MentoringPostMapper">
    <select id="findAll" resultType="fashionmanager.baek.develop.dto.SelectAllPostDTO">
        SELECT
                num
              , title
              , author_num
          FROM mentoring_post
         ORDER BY num desc;
    </select>
</mapper>